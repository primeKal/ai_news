# Telegram Bot Token — get from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Telegram Channel ID — e.g. -1001234567890 or @your_channel_username
TELEGRAM_CHANNEL_ID=your_channel_id_here

# Schedule time (24h format, UTC) — only used with --schedule flag
SCHEDULE_HOUR=8
SCHEDULE_MINUTE=0
